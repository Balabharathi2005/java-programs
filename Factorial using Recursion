import java.util.Scanner;
    class factorial{
        static int Fact(int n){
            if(n==0){
                return 1;
            }
            else{
               return (n*Fact(n-1));
            }
        }
         public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a number: ");
        int fact=1;
        int n=sc.nextInt();
        fact=Fact(n);
        
        System.out.println(fact);
         }
    }
